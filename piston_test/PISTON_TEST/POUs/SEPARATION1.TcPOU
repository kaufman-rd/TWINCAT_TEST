<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.12">
  <POU Name="SEPARATION1" Id="{0a3315b2-61c0-4450-961e-215e0d8b923c}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM SEPARATION1
VAR
	tDelay: TON;
	bDELAY: BOOL:=FALSE;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[tDelay(IN:=bDELAY);
CASE eSEPARATE1 OF
	E_STATION_STATE.uninit:
		eSEPARATE1:= E_STATION_STATE.idle;

	E_STATION_STATE.idle:
		bS2_RUNNING:=FALSE;
			

	E_STATION_STATE.do_action1://delay and open piston3
		tDELAY.PT:=T#1S;
		bDELAY:=TRUE;
		IF tDELAY.Q THEN
			bDELAY:=FALSE;
			fbPISTON[3].A_OPEN();
		END_IF
		
		IF fbPISTON[3].PistonStatus= ePistonStatus.OPENED THEN
			eSEPARATE1:=E_STATION_STATE.do_action2;
		END_IF
		
		E_STATION_STATE.do_action2://delay and open piston4
		tDELAY.PT:=T#1S;
		bDELAY:=TRUE;
		IF tDELAY.Q THEN
			bDELAY:=FALSE;
			fbPISTON[4].A_OPEN();
		END_IF
		
		IF fbPISTON[4].PistonStatus= ePistonStatus.OPENED THEN
			eSEPARATE1:=E_STATION_STATE.do_action3;
		END_IF
	
	E_STATION_STATE.do_action3://delay and close pistons 4 and 3
		tDELAY.PT:=T#2S;		
		bDELAY:=TRUE;
		IF tDELAY.Q THEN
			bDELAY:=FALSE;
			fbPISTON[4].A_CLOSE();
			fbPISTON[3].A_CLOSE();
		END_IF
		IF fbPISTON[4].PistonStatus= ePistonStatus.CLOSED AND fbPISTON[3].PistonStatus= ePistonStatus.CLOSED THEN
			eSEPARATE1:=E_STATION_STATE.do_action4;
		END_IF
	
	E_STATION_STATE.do_action4://delay
		tDELAY.PT:=T#2S;
		bDELAY:=TRUE;
		IF tDELAY.Q THEN
			bDELAY:=FALSE;
			eSEPARATE1:=E_STATION_STATE.done;
		END_IF
		
	
	E_STATION_STATE.done:
		bS2_RUNNING:=FALSE;
		
	E_STATION_STATE.pause:
		//paused

END_CASE]]></ST>
    </Implementation>
  </POU>
</TcPlcObject>